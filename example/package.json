{
  "name": "ts-kaiju-example",
  "version": "0.0.1",
  "private": true,

  "scripts": {
    "lint": "node_modules/tslint/bin/tslint -c build/tslint.json 'src/**/*.ts'",
    "build": "npm run lint && webpack -p --config build/prod.js",
    "watch": "webpack --watch --config build/watch.js",
    "watch-sync": "browser-sync start --files \"public/**\" --proxy localhost:8000 & npm run watch"
  },

  "dependencies": {
    "typescript": "beta",
    "kaiju": "0.10.2",
    "snabbdom": "0.5.2",
    "abyssa": "8.0.2",
    "immupdate": "0.3.0",
    "animejs": "1.1.0"
  },

  "devDependencies": {
    "webpack": "1.13.1",
    "ts-loader": "0.7.2",
    "css-loader": "0.23.1",
    "style-loader": "0.12.3",
    "stylus-loader": "2.1.1",
    "stylus": "0.54.5",
    "extract-text-webpack-plugin": "1.0.1",
    "tslint": "3.15.1"
  }
}
